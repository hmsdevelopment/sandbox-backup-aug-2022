      <loglevel>DEBUG</loglevel>
      <status>NOTSCHEDULED</status>
      <title>REN Emails- WebBee - Every Hour</title>
      <recurrence>
        <daily>
          <everyxdays>1</everyxdays>
          <repeat>PT1H</repeat>
          <startdate>2016-10-23</startdate>
          <starttime>12:00:00Z</starttime>
        </daily>
      </recurrence>
    </scriptdeployment>
  </scriptdeployments>
  <libraries>
    <library>
      <scriptfile>[/SuiteScripts/Suitescript/Webbee Scripts/Crypto/Crypto.js]</scriptfile>
    </library>
  </libraries>
